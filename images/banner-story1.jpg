const editorTooltip = document.getElementsByClassName("editor-tooltip")[0];
const globalNavLinks = document.querySelectorAll('[data-trackNav]');
for (var i = 0; i < globalNavLinks.length; i++) {
  globalNavLinks[i].addEventListener('click', event => {
    if(event.currentTarget.href) event.preventDefault();
    if (window.combinedGtmTracking) {
      window.combinedGtmTracking.gtmPushTracking('navigationInteraction', {
        'navigation_interaction': 'Header_Link_Click',
        'navigation_selection': `sub_nav_video_editor_${event.currentTarget.dataset.tracknav}`,
        'ui_element_location': 'header'
      });
    }
    if(event.currentTarget.href) window.open(event.currentTarget.href, '_blank');
  });
}

editorTooltip.style.display = showTooltip ? "block" : "none";
if(showTooltip) {
  const createVideos = document.getElementsByClassName("video-editor")[0];
  const close = document.getElementsByClassName("close-tt")[0];
  const projLink = document.getElementsByClassName("projLink")[0];
  let waitForGlobal = (key, callback) => {
    if (window[key]) {
      callback();
    } else {
      setTimeout(() => {
        waitForGlobal(key, callback);
      }, 100);
    }
  };
  let track = (interaction) => {
    if (window.combinedGtmTracking) {
      window.combinedGtmTracking.gtmPushTracking('notificationTooltipInteraction', {
        'tooltip_interaction': interaction,
        'tooltip_selection': 'nav_tooltip_drafts_reminder',
        'ui_element_location': 'nav_header_tooltip_video_editor'
      });  
    }
  }
  waitForGlobal("combinedGtmTracking", () => { track('Viewed'); });
  // Needed as the tooltip and the create menu overlap with each other. 
  let closeTooltip = () => {
    localStorage.setItem('show_editor_draft_reminder', false);
    editorTooltip.style.display = "none";
  }
  if (close)
    close.addEventListener('click', event => {
      closeTooltip();
      track('Close');
    });
  if (projLink)
    projLink.addEventListener('click', event => {
      event.preventDefault();
      closeTooltip();
      track('Click'); 
      location.href = projLink.getAttribute('href');
